methode decimalToInput2
  f;f;tr;f
  wenn nicht istMarke dann {0,1,7}
    f
    wenn istMarke dann {0}
      toInput2;clear;tr
    sonst
      tr;f;f
      wenn istMarke dann {7}
        toInput2
        clear
        tr
        markeSetzen;f
        markeSetzen;f
        markeSetzen
      sonst {1}
        toInput2;clear
        markeSetzen
      *wenn
    *wenn
  sonst {2,3,4,5,6,8,9}
    f
    wenn nicht istMarke dann {3}
      toInput2;clear;tr
      markeSetzen;f
      markeSetzen
    sonst {2,4,5,6,8,9}
      tl;f
      wenn istMarke dann {2,6,8}
        t2;f;f
        wenn istMarke dann {6,8}
          tr;f;f
          wenn istMarke dann {8}
            toInput2;clear;tr
            f;f;f
            markeSetzen
          sonst {6}
            toInput2;clear;tr
            f
            markeSetzen;f
            markeSetzen
          *wenn
        sonst {2}
          toInput2;clear;tr
          f;markeSetzen
        *wenn
      sonst {4,5,9}
        f
        wenn istMarke dann {5,9}
          t2;f;f;f;tr;f;f
          wenn istMarke dann {9}
            toInput2;clear;tr
            markeSetzen
            f;f;f
            markeSetzen
          sonst {5}
            toInput2;clear;tr
            markeSetzen
            f;f
            markeSetzen
          *wenn
        sonst {4}
          toInput2;clear;tr
          f;f
          markeSetzen
        *wenn
      *wenn
    *wenn
  *wenn
*methode
